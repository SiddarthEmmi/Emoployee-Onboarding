        <name>Auto fill EMail</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') return;

    var ga = new GlideAjax('GetUserEmail');
    ga.addParam('sysparm_name', 'getEmail');
    ga.addParam('sysparm_user_id', newValue); // 'name' is reference (sys_id)

    ga.getXMLAnswer(function(response) {
        if (response) {
            g_form.setValue('email', response);
        } else {
            g_form.setValue('email', '');
        }
    });
}
]]></script>
